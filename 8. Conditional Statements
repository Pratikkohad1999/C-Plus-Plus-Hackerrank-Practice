#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    int apple;
    cin >> apple;
    
    if (apple == 1) 
    { 
        cout << "one"; 
    }
    else if (apple == 2) { 
        cout << "two"; 
        }
    else if (apple == 3) 
    { cout << "three"; 
    }
    else if (apple== 4) 
    { cout << "four"; 
    }
    else if (apple == 5) 
    { cout << "five"; 
    }
    else if (apple == 6) 
    { cout << "six"; 
    }
    else if (apple == 7) 
    { cout << "seven"; 
    }
    else if (apple == 8) 
    { cout << "eight"; 
    }
    else if (apple== 9) 
    { cout << "nine"; 
    }
    else { cout << "Greater than 9"; }

    return 0;
}
